proc ModulesHelp { } {

    puts stderr "This module adds dos2unix 7.3 to your environment."
    puts stderr "Text format converters dos2unix, unix2dos, mac2unix, unix2mac."
    puts stderr ""

}

module-whatis "Adds dos2unix 7.3 to your environment. Text format converters dos2unix, unix2dos, mac2unix, unix2mac."

# Add binary to PATH
prereq gcc-libs

conflict dos2unix

set root 			/shared/ucl/apps/dos2unix/7.3/gnu-4.9.2
prepend-path PATH    		$root/bin
prepend-path MANPATH		$root/share/man
