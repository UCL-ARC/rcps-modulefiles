proc ModulesHelp { } {
    puts stderr "Adds ea-utils to your environment."
    puts stderr ""
}

module-whatis "Adds ea-utils to your environment."

conflict ea-utils
prereq gcc-libs
prereq gsl/1.16/gnu-4.9.2

set prefix /shared/ucl/apps/ea-utils/822

prepend-path PATH                $prefix/bin
