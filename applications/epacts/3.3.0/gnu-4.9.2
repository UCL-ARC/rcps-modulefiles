#%Module -*- tcl -*-
##
## dot modulefile
##
## EPACTS 3.3.0 compiled with GCC 4.9.2

# New Legion modules layout

proc ModulesHelp { } {
    puts stderr "Adds EPACTS 3.3.0 to your environment."
    puts stderr ""
    puts stderr "EPACTS (Efficient and Parallelizable Association Container"
    puts stderr "Toolbox) is a versatile software pipeline to perform various"
    puts stderr "statistical tests for identifying genome-wide association from"
    puts stderr "sequence data "
    puts stderr ""
    puts stderr "Directory: /shared/ucl/apps/EPACTS/3.3.0/"
}

module-whatis "Adds EPACTS 3.3.0 to your environment."

# Location of EPACTS:

set prefix /shared/ucl/apps/EPACTS/3.3.0

prereq gcc-libs
prereq compilers/gnu/4.9.2
prereq r/recommended
prereq gnuplot/5.0.1
prereq groff/1.22.3/gnu-4.9.2

conflict epacts

prepend-path PATH $prefix/bin
setenv EPACTS_DIR $prefix
