proc ModulesHelp { } {

    puts stderr "This module adds the Molpro package to your environment."
    puts stderr ""
}

module-whatis "Adds Molpro to your environment"


conflict molpro
prereq gcc-libs
prereq compilers/gnu/4.9.2
prereq mpi/openmpi/1.8.4/gnu-4.9.2

set              root              /shared/ucl/apps/molpro/2012.1.25/molprop_2012_1_Linux_x86_64_i8

prepend-path     PATH              $root/bin
prepend-path     LD_RUN_PATH       $root/lib
prepend-path     LD_LIBRARY_PATH   $root/lib
