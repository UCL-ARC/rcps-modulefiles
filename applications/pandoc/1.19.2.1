# New Legion modules layout

proc ModulesHelp { } {
    puts stderr "Adds pandoc Version 1.19.2.1 to your environment."
    puts ""
    puts stderr "pandoc is a document converter with support for a large selection of"
    puts stderr "document formats."
    puts stderr ""
    puts stderr "Directory: /shared/ucl/apps/pandoc"
}

module-whatis "Adds pandoc Version 1.19.2.1 to your environment."

# Location of pandoc:

set prefix /shared/ucl/apps/pandoc

prereq gcc-libs
conflict pandoc

prepend-path PATH		$prefix/bin
