proc ModulesHelp { } {
    puts stderr "Adds Tophat 2.1.0 to your environment."
    puts stderr ""
}

module-whatis "Adds Tophat 2.1.0 to your environment."

conflict tophat
prereq gcc-libs
prereq python/2.7.9
prereq bowtie
prereq bowtie2

# Location of Bowtie
set prefix /shared/ucl/apps/tophat/2.1.0/tophat-2.1.0.Linux_x86_64

prepend-path PATH                $prefix
