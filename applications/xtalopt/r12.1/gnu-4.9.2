prereq xorg-utils/X11R7.7
prereq qt/5.12.1/gnu-4.9.2
prereq eigen/3.2.5/gnu-4.9.2
prereq qwt/6.1.4/gnu-4.9.2


prepend-path PATH		$prefix/bin
