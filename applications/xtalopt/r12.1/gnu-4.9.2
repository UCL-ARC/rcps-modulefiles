proc ModulesHelp { } {
    puts stderr "Adds XtalOpt r12.1 to your environment."
    puts stderr ""
    puts stderr ""
    puts stderr "Directory: /shared/ucl/apps/XtalOpt/r12.1/gnu-4.9.2/"
