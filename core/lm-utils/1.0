#%Module -*- tcl -*-
##
## modulefile
##
## lm-utils - license manager utilities
##
## Updated September 2019 for new scripts location.

proc ModulesHelp { } {

  puts stderr "\tAdds utilities to check license manager status to your environment."
}

module-whatis "adds utilities to check license manager status to your environment."

conflict lm-utils

set prefix /shared/ucl/apps/lm-utils

prepend-path PATH               $prefix/scripts
