proc ModulesHelp { } {
  puts stderr "\tAdds Cmake 3.7.2 to your environment variables,"
}

module-whatis "adds Cmake 3.7.2 compilers to your environment variables"

conflict cmake
prereq gcc-libs

prepend-path     PATH                 /shared/ucl/apps/cmake/3.7.2/gnu-4.9.2/bin
prepend-path     MANPATH              /shared/ucl/apps/cmake/3.7.2/gnu-4.9.2/share/man
