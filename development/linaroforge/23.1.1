#%Module -*- tcl -*-
## module_made (v2)
proc ModulesHelp { } {
  puts stderr {Linaro Forge (previously ArmForge) is a collection of parallel profiling and debugging tools for x86_64 and ARM processors.}
}
module-whatis {Linaro Forge (previously ArmForge) is a collection of parallel profiling and debugging tools for x86_64 and ARM processors.}

conflict linaroforge

set prefix /shared/ucl/apps/linaroforge/23.1.1
prepend-path CMAKE_PREFIX_PATH $prefix
prepend-path PATH $prefix/bin
prepend-path LIBRARY_PATH $prefix/lib
prepend-path LD_RUN_PATH $prefix/lib
prepend-path LD_LIBRARY_PATH $prefix/lib

