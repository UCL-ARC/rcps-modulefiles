#%Module -*- tcl -*-
#
# Qwt 6.2.0
#
#                                                      BAA February 2019
# Updated to use working Qt 5.12.1 March 2019
# Updated to v 6.2.0 July 2024

proc ModulesHelp { } {

    puts stderr "This module adds Qwt 6.2.0 to your environment."
    puts stderr ""
    puts stderr "Qwt: Qt Widgets for Technical applications."
    puts stderr ""

}

module-whatis "Adds Qwt 6.2.0 to your environment."

prereq gcc-libs
prereq compilers/gnu/4.9.2
prereq qt/5.12.1/gnu-4.9.2

conflict qwt

set root 			/shared/ucl/apps/qwt/6.2.0/gnu-4.9.2/qwt-6.2.0

prepend-path QT_PLUGIN_PATH     $root/plugins
prepend-path LD_LIBRARY_PATH	$root/lib
prepend-path LIBRARY_PATH	$root/lib
prepend-path CPATH		$root/include
prepend-path INCLUDE_PATH	$root/include
prepend-path MANPATH            $root/doc/man

setenv QWT_ROOT $root

prepend-path CMAKE_PREFIX_PATH $root
