proc ModulesHelp { } {
  puts stderr "\tAdds APR 1.5.2 to your environment variables,"
}

module-whatis "adds APR 1.5.2 to your environment variables"

conflict apr
prereq gcc-libs

#APR
prepend-path     PATH                 /shared/ucl/apps/apr/1.5.2/bin
prepend-path     LD_LIBRARY_PATH      /shared/ucl/apps/apr/1.5.2/lib
prepend-path     LIBRARY_PATH         /shared/ucl/apps/apr/1.5.2/lib
prepend-path     CPATH                /shared/ucl/apps/apr/1.5.2/include

prepend-path     CMAKE_PREFIX_PATH    /shared/ucl/apps/apr/1.5.2
