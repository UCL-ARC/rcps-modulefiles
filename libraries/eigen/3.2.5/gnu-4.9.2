proc ModulesHelp { } {
  puts stderr "\tAdds Eigen 3.2.5 GCC 4.9.2 compilers to your environment variables."
}

module-whatis "adds Eigen for GCC 4.9.2 compilers to your environment variables"

conflict eigen
prereq gcc-libs

set              root              /shared/ucl/apps/eigen/3.2.5/gnu-4.9.2

prepend-path     CPATH             $root/include
prepend-path     INCLUDE_PATH      $root/include

prepend-path CMAKE_PREFIX_PATH  $root
