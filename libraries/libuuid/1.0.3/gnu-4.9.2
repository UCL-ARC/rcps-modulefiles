#%Module -*- tcl -*-
#
# libuuid 1.0.3
#
#                                                      H.K. Feb 2016

proc ModulesHelp { } {

    puts stderr "This module adds a static libuuid 1.0.3 to your environment."
    puts stderr ""
}

module-whatis "Adds a static libuuid 1.0.3 to your environment."

prereq gcc-libs

conflict libuuid

set root                      	/shared/ucl/apps/libuuid/1.0.3/gnu-4.9.2
prepend-path LD_LIBRARY_PATH    $root/lib
prepend-path LIBRARY_PATH       $root/lib
prepend-path INCLUDE_PATH	$root/include
prepend-path CPATH		$root/include
prepend-path PKG_CONFIG_PATH	$root/lib/pkgconfig
prepend-path CMAKE_PREFIX_PATH  $root
